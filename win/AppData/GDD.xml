<?xml version="1.0"?><!--global data dictionary-->
<GDD>
  <SiteSetting>
    <Site name="site1" address="localhost" port="1234"></Site>
    <Site name="site2" address="localhost" port="2345"></Site>
    <Site name="site3" address="localhost" port="3456"></Site>
  </SiteSetting>
  <TableSetting count="3">
    <Table name="Student" fragmentation="horizontal" sites="3">
      <Site name="site1" conditions="1">
        <Range var="id">
          <Low equal="yes">MIN</Low>
          <Up equal="no">10</Up>
        </Range>
      </Site>
      <Site name="site2" conditions="2">
        <Range var="id">
          <Low equal="yes">10</Low>
          <Up equal="no">20</Up>
        </Range>
      </Site>
      <Site name="site3" conditions="1">
        <Range var="id">
          <Low equal="yes">20</Low>
          <Up equal="no">MAX</Up>
        </Range>
      </Site>
    </Table>
    <Table name="Course" fragmentation="horizontal" sites="2">
      <Site name="site1" conditions="1">
        <Range var="id">
          <Low equal="yes">MIN</Low>
          <Up equal="no">4</Up>
        </Range>
      </Site>
      <Site name="site2" conditions="1">
        <Range var="id">
          <Low equal="yes">4</Low>
          <Up equal="no">Max</Up>
        </Range>
       </Site>
    </Table>
    <Table name="Exam" fragmention="horizontal" sites="3">
      <Site name="site1" conditions="1">
        <Range var="id">
          <Low equal="yes">MIN</Low>
          <Up equal="no">10</Up>
        </Range>
      </Site>
      <Site name="site2" conditions="2">
        <Range var="id">
          <Low equal="yes">10</Low>
          <Up equal="no">20</Up>
        </Range>
      </Site>
      <Site name="site3" conditions="1">
        <Range var="id">
          <Low equal="yes">20</Low>
          <Up equal="no">MAX</Up>
        </Range>
      </Site>
    </Table>
  </TableSetting>
</GDD>